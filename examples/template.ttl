@prefix : <http://aksw.org/deer/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:d1           a                  :Dataset ;
		          :hasUri			       <http://dbpedia.org/resource/Berlin> ;
		          :fromEndPoint	     <http://dbpedia.org/sparql> .

:d2           a                  :Dataset .
:d3           a                  :Dataset .
:d4           a                  :Dataset .
:d5	          a                  :Dataset ;
		          :outputFile		     "output1.ttl" ;
		          :outputFormat	     "Turtle" .

:merge        a                  :Operator ;
              :implementedIn     :org.aksw.deer.enrichment.merge.MergeEnrichmentOperator ;

:conf         a                  :Operator ;
              :implementedIn     :org.aksw.deer.enrichment.authorityconformation.AuthorityConformationEnrichmentOperator ;
		          :sourceSubjectAuthority  "http://dbpedia.org" ;
		          :targetSubjectAuthority  "http://deer.org" .

:pconf         a                  :Operator ;
              :implementedIn     :org.aksw.deer.enrichment.predicateconformation.PredicateConformationEnrichmentOperator ;
		          rdfs:label		     "Conformation" ;
		          :propertyMapping   ( [ :source "" ; :target "" ] [ :source "" ; :target "" ] )

:split        a                  :Operator ;
              :implementedIn     :org.aksw.deer.enrichment.clone.CloneEnrichmentOperator ;

:filter       a                  :Operator ;
              :implementedIn     :org.aksw.deer.enrichment.clone.CloneEnrichmentOperator ;
              :selectors         ( [ rdf:subject _:1 ; rdf:predicate "" ; rdf:object _:1 ] [ rdf:predicate "" ] )